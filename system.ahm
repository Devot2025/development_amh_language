@ note : type abs rule
@ array >> string >> double > float > int > char > ::class:: > virtuals > none
out{
   +{
      @ ______write call to print
      putsln(str){
          ______write(fld:stdout, str);
          ______write(fld:stdout, '\n');
      }
      putsln(str, n){
          ______write(fld:stdout, str, n);
          ______write(fld:stdout, '\n');
      }
   }
   -{
       stdout = 1;
   }
}

sys{
   +{
       out = <out>;
   }
   -{

   }
}
